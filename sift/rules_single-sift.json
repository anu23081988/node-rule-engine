[
    {
        "conditions": {
            "$and": [
                {
                    "id": {
                        "$exists": true
                    }
                }
            ]
        },
        "event": {
            "type": "id-exists"
        }
    },
    {
        "conditions": {
            "$and": [
                {
                    "name": {
                        "$regex": "^Mr\\."
                    }
                }
            ]
        },
        "event": {
            "type": "name-startsWith-Mr"
        }
    },
    {
        "conditions": {
            "$and": [
                {
                    "email": {
                        "$regex": "@gmail\\.com$"
                    }
                }
            ]
        },
        "event": {
            "type": "email-endsWith-gmail"
        }
